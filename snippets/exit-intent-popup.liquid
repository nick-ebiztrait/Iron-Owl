(function(){
  const OVERLAY = document.getElementById("exit-overlay");
  const CLOSE_BTN = document.querySelector(".exit-close");
  const COOKIE_KEY = "exit_intent_shown";

  // --- Cookie helpers ---
  function setCookie(name, value, hours) {
    const d = new Date();
    d.setTime(d.getTime() + (hours*60*60*1000));
    document.cookie = `${name}=${value};expires=${d.toUTCString()};path=/`;
  }
  function getCookie(name) {
    return document.cookie.split("; ").find(row => row.startsWith(name+"="))?.split("=")[1];
  }

  // --- Show / hide popup ---
  function showPopup() {
    if (getCookie(COOKIE_KEY)) return; // show only once

    OVERLAY.style.display = "flex";
    OVERLAY.setAttribute("aria-hidden", "false");
    document.documentElement.style.overflow = "hidden";

    document.getElementById("popupContent").innerHTML = "Loading...";

    fetch("https://estagingserver.com/ironowlhome/public/index.php/popup/create") // must allow CORS
      .then(res => res.text())
      .then(data => {
        document.getElementById("popupContent").innerHTML = data;
      })
      .catch(() => {
        document.getElementById("popupContent").innerHTML = "Error loading content.";
      });

    setCookie(COOKIE_KEY, "true", 24); // mark as shown
  }

  function hidePopup() {
    OVERLAY.style.display = "none";
    OVERLAY.setAttribute("aria-hidden", "true");
    document.documentElement.style.overflow = "";
  }

  // --- Desktop exit intent ---
  function onMouseOut(e) {
    if (!e.relatedTarget && e.clientY <= 0) {
      showPopup();
      window.removeEventListener("mouseout", onMouseOut);
    }
  }

  // --- Mobile exit intent ---
  function onMobileBack() {alert('back');
    showPopup();
    window.removeEventListener("popstate", onMobileBack);
  }

  function onMobileScroll() {alert(5);
    if (window.scrollY <= 50) { // near top
      showPopup();
      window.removeEventListener("scroll", onMobileScroll);
    }
  }

  // --- Close button ---
  CLOSE_BTN.addEventListener("click", hidePopup);

  // --- Init ---
  document.addEventListener("DOMContentLoaded", () => {alert(3);
    if (!getCookie(COOKIE_KEY)) {
      // Desktop
      window.addEventListener("mouseout", onMouseOut);

      // Mobile detection
      if (/Mobi|Android/i.test(navigator.userAgent)) {alert(4);
        // Back button
        window.history.pushState({popup: true}, "");
        window.addEventListener("popstate", onMobileBack);

        // Scroll-to-top trigger
        window.addEventListener("scroll", onMobileScroll, { passive: true });
      }
    }
  });
  console.log('ffffffffffffff');
})();

